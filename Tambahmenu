    private void JenisActionPerformed(java.awt.event.ActionEvent evt) {                                      
        // TODO add your handling code here:
        switch (Jenis.getSelectedIndex()){
            case 1 : 
                Tambahmenu2 t2 = new Tambahmenu2();
                t2.setVisible(true);
                t2.pack();
                t2.setLocationRelativeTo(null);
                t2.setDefaultCloseOperation(Tambahmenu.EXIT_ON_CLOSE);
                break;
            case 2 :
                Tambahmenu3 t3 = new Tambahmenu3();
                t3.setVisible(true);
                t3.pack();
                t3.setLocationRelativeTo(null);
                t3.setDefaultCloseOperation(Tambahmenu.EXIT_ON_CLOSE);
                break;
            case 3 :
                Tambahmenu4 t4 = new Tambahmenu4();
                t4.setVisible(true);
                t4.pack();
                t4.setLocationRelativeTo(null);
                t4.setDefaultCloseOperation(Tambahmenu.EXIT_ON_CLOSE);
                break;
            case 4 :
                Tambahmenu5 t5 = new Tambahmenu5();
                t5.setVisible(true);
                t5.pack();
                t5.setLocationRelativeTo(null);
                t5.setDefaultCloseOperation(Tambahmenu.EXIT_ON_CLOSE);
                break;
        }
                
    }                                     

