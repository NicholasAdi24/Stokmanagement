    private void savebuttonActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        try {
            st = cn.createStatement();
            if(namamakanan1.getText().equals("")  ){
                JOptionPane.showMessageDialog(null, "Nama tidak boleh kosong", "Validasi Data", JOptionPane.INFORMATION_MESSAGE);
                return;
            }
            if (savebutton.getText()== "SAVE"){
                String cek = "SELECT * FROM data_stok WHERE Nama Makanan = '"+namamakanan1.getText()+"'";
                rs = st.executeQuery(cek);
                if (rs.next()){
                    JOptionPane.showMessageDialog(null, "Nama makanan ini sudah ada");   
                }else{
                    String sql = "INSERT INTO data_stok VALUES ('"+ namamakanan1.getText()+"','"+
                            (int) jumnasi.getValue()+"','"+
                            (int) jumbwmr.getValue()+"','"+
                            (int) jumbwp.getValue()+"','"+
                            (int) jumbwb.getValue()+"','"+
                            (int) jumgrm.getValue()+"','"+
                            (int)jumgl.getValue()+"','"+
                            (int)jumryay.getValue()+"','"+
                            (int)jumryspi.getValue()+"','"+
                            (int)jumktg.getValue()+"','"+
                            (int)jumcb.getValue()+"','"+
                            (int)jummgr.getValue()+"','"+
                            (int)jumwrtl.getValue()+"','"+
                            (int)jumdnbw.getValue()+"','"+
                            (int)jumdgay.getValue()+"','"+
                            (int)jumdgsp.getValue()+"')";
                    
                    st.executeUpdate(sql);
//                    st.executeUpdate(sqlp);
                    JOptionPane.showMessageDialog(null, "Data berhasil disimpan");
                            
                }
            }
        } catch (Exception e) {
        }
        Mainmenu mm = new Mainmenu();  
        
        mm.makanan1 = namamakanan1.getText();
        mm.setVisible(true);
        mm.pack();
        mm.setLocationRelativeTo(null);
        mm.setDefaultCloseOperation(Tambahmenu.EXIT_ON_CLOSE);
        
    }                                          

